{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SimpleAuthenticationServiceDatabase": "Host=postgres;Port=5432;Database=simple_authentication_service;Username=sa;Password=Test123456789;"
  },
  "TokenOptions": {
    "AccessTokenOptions": {
      "Issuer": "http://localhost:5000/",
      "Audience": "http://localhost/",
      "PrivateRsaCertificate": "",
      "PublicRsaCertificate": "",
      "LifeTimeInSeconds": 300
    },
    "RefreshTokenOptions": {
      "Length": 512,
      "LifeTimeInMinutes": 120
    }
  },
  "OutboxPatternOptions": {
    "IntervalInSeconds": 10,
    "BatchSize": 10
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "/logs/log-.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  }
}
